{
    "name": "license-plate-judas",
    "main": "node_modules/expo/AppEntry.js",
    "version": "0.1.1",
    "private": true,
    "repository": "https://github.com/leap-spark/license-plate-judas",
    "scripts": {
        "start": "cross-env expo start",
        "android": "cross-env expo start --android",
        "ios": "cross-env expo start --ios",
        "eject": "cross-env expo eject",
        "lint": "cross-env tslint **/*.ts",
        "tsc": "cross-env tsc",
        "clean": "cross-env rimraf build",
        "build": "yarn run clean && yarn run tsc --",
        "watch": "yarn run build -- -w",
        "watchAndRunAndroid": "cross-env concurrently \"yarn run watch\" \"yarn run android\"",
        "buildRunAndroid": "yarn run build && yarn run watchAndRunAndroid ",
        "watchAndRunIOS": "cross-env concurrently \"yarn run watch\" \"yarn run ios\"",
        "buildRunIOS": "yarn run build && yarn run watchAndRunIOS ",
        "watchAndStart": "cross-env concurrently \"yarn run watch\" \"yarn run start\"",
        "buildAndStart": "yarn run build && yarn run watchAndStart"
    },
    "dependencies": {
        "cross-env": "^5.2.0",
        "env2": "^2.2.2",
        "expo": "^30.0.1",
        "firebase": "^5.5.3",
        "react": "16.3.1",
        "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
        "react-native-dotenv": "^0.2.0",
        "react-native-paper": "^2.1.3",
        "react-native-vector-icons": "^6.0.2",
        "react-navigation": "^2.17.0",
        "react-navigation-material-bottom-tabs": "^0.4.0",
        "sentry-expo": "^1.10.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.6",
        "eslint": "^4.19.1",
        "eslint-plugin-react": "^7.11.1"
    }
}
